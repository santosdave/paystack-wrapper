{
  "name": "santosdave/laravel-paystack",
  "version": "1.0.0",
  "description": "A comprehensive and secure Laravel wrapper for Paystack payment gateway",
  "keywords": [
    "laravel",
    "paystack",
    "payment",
    "api",
    "gateway",
    "wrapper"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "santosdave",
      "email": "51959957+santosdave@users.noreply.github.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.5",
    "illuminate/http": "^9.0|^10.0|^11.0",
    "laravel/framework": "^10.0",
    "monolog/monolog": "^3.8",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "SantosDave\\Paystack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SantosDave\\Paystack\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SantosDave\\Paystack\\PaystackServiceProvider"
      ],
      "aliases": {
        "Paystack": "SantosDave\\Paystack\\Facades\\Paystack"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/pint"
  }
}
